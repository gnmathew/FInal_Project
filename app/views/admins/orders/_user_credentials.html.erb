<%= form_with(model: order, url: url, method: method, data: {turbo: false}) do |form| %>
  <%= form.label :email,"Client User Email" %>
  <%= form.text_field :email,value: @client.email ,class:"form-control mb-2 mt-2", disabled: true %>
  <%= form.hidden_field :user_id, value: params[:user_id] %>
  <%= form.label :coin,"Promoter Email" %>
  <%= form.text_field :parent_id , value: @client.parent&.email, class:"form-control mb-2",placeholder: "Promoter email", disabled: true %>
  <%= form.label :coin,"Coins" %>
  <%= form.number_field :coins ,value: @client.coins, class:"form-control mb-2", disabled: true %>
<% end %>